am start -n com.pubg.krmobile/com.epicgames.ue4.SplashActivity


if [ -d /storage/emulated/0/Android/obb/com.tencent.ig ]
then 
awk '/^com.tencent.ig/ {print $2}' /data/system/packages.list > /data/media/0/g
g=$(cat /data/media/0/g)
su -c iptables -I INPUT -m owner --uid-owner $g -p tcp -j DROP &>/dev/null;
su -c iptables -I OUTPUT -m owner --uid-owner $g -p tcp -j DROP &>/dev/null;
su -c ip6tables -I INPUT -m owner --uid-owner $g -p tcp -j DROP &>/dev/null;
su -c ip6tables -I OUTPUT -m owner --uid-owner $g -p tcp -j DROP &>/dev/null;
iptables -I INPUT -m owner --uid-owner $g -p tcp -j DROP &>/dev/null;
iptables -I OUTPUT -m owner --uid-owner $g -p tcp -j DROP &>/dev/null;
ip6tables -I INPUT -m owner --uid-owner $g -p tcp -j DROP &>/dev/null;
ip6tables -I OUTPUT -m owner --uid-owner $g -p tcp -j DROP &>/dev/null;
else
echo
fi
